<%- include('../head', {title: 'Services'}); %>
<section>
  <center>
    <h1><%= service.name %></h1>
    <h3>$<%= service.price %>.00</h3>
    <a href="javascript:cartAdd({name:'<%= service.name %>',price: <%= service.price %>});" class="add-btn">Add to Cart</a><br>
    <a href="javascript:cartRemove({name:'<%= service.name %>'});" class="remove-btn">Remove From Cart</a>
    <script src="/js/cart/add.js"></script>
    <script src="/js/cart/remove.js"></script>
  </center>
</section>
<%- include('../foot'); %>